.content {
  height: 100%vh;
  width: 100%;
  background-color: #F2F2F2;
  /* 	结束按钮 */
}
.content .worp {
    width: 100%;
    background-color: #FFFFFF;
    padding-top: 0 !important;
    margin: 0 0 30rpx 0;
}
.content .worp ._hr {
      height: 1rpx;
      border: none;
      border-top: 1px solid #F4F4F4;
      width: 97%;
      margin: 0 auto;
}
.content .worp image {
      width: 100%;
      height: 364rpx;
}
.content .worp .theme {
      font-size: 24rpx;
      color: #333333;
      margin: 20rpx 0 20rpx 23rpx;
}
.content .worp .line {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      margin-left: 23rpx;
}
.content .worp .line image {
        width: 32rpx;
        height: 32rpx;
        margin-right: 28rpx;
}
.content .worp .line text {
        font-size: 28rpx;
        color: #666666;
        margin-right: 20rpx;
}
.content .worp .line .theme1 {
        font-size: 24rpx;
        color: #333333;
        margin: 20rpx 0 20rpx 0rpx;
}
.content .worp .price {
      font-size: 25rpx;
      font-weight: bold;
      color: #D80000;
      margin: 20rpx 0 0 23rpx;
      letter-spacing: 3rpx;
}
.content .worp .count {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      margin-left: 23rpx;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
}
.content .worp .count text {
        font-size: 26rpx;
        color: #F2221F;
}
.content .worp .count view {
        width: 44rpx;
        height: 44rpx;
        text-align: center;
        font-size: 21rpx;
        color: #FFFFFF;
        line-height: 44rpx;
        background-color: #D80000;
        border-radius: 5rpx;
        margin: 10rpx 0;
}
.content .worp .top {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      margin: 0 23rpx;
      padding: 20rpx 0;
}
.content .worp .top text:first-child {
        font-size: 27rpx;
        color: #000000;
}
.content .worp .top text:last-child {
        font-size: 23rpx;
        color: #EF7C38;
}
.content .worp .personnel-list {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-justify-content: space-around;
              justify-content: space-around;
}
.content .worp .personnel-list .list image {
        width: 96rpx;
        height: 96rpx;
        border-radius: 50%;
}
.content .worp .personnel-list .list view {
        font-size: 23rpx;
        text-align: center;
}
.content .p-t {
    font-size: 28rpx;
}
.content .image {
    margin-top: 30rpx;
    width: 100%;
    height: 988rpx;
}
.content .underway {
    background: #FFFFFF;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    padding: 30rpx 0;
}
.content .underway button:first-child {
      width: 250rpx;
      height: 80rpx;
      font-size: 30rpx;
      color: #FFFFFF;
      text-align: center;
      line-height: 80rpx;
      background-color: #D80000;
      border: none;
      border-radius: 40rpx;
      letter-spacing: 10rpx;
}
.content .underway button:last-child {
      width: 250rpx;
      height: 80rpx;
      font-size: 30rpx;
      color: #FFFFFF;
      text-align: center;
      line-height: 80rpx;
      background-color: #EF7C38;
      border: none;
      border-radius: 40rpx;
      letter-spacing: 10rpx;
}
.content .finish {
    background: #FFFFFF;
    padding: 30rpx 0;
    display: none;
}
.content .finish button {
      width: 600rpx;
      height: 80rpx;
      font-size: 30rpx;
      color: #FFFFFF;
      text-align: center;
      line-height: 80rpx;
      background-color: #999999;
      border: none;
      border-radius: 40rpx;
      letter-spacing: 10rpx;
}

