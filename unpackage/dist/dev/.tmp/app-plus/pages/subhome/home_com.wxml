<view class="content"><view class="status_bar"></view><view class="content_top"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="top_returns" bindtap="__e"><image src="/static/image/com_page/returns.png" mode="widthFix"></image></view><view class="top_search">{{''+title+''}}</view><view class="top_img"><image src="/static/image/com_page/search.png" mode="widthFix"></image></view></view><view class="tab_list"><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="list_all" bindtap="__e"><view>{{''+v_test+''}}</view><image class="{{['all_img',show===false?'tran_none':show===true?'tran_show':'']}}" src="../../static/image/index/down.png" mode="widthFix"></image></view><view data-event-opts="{{[['tap',[['condition']]]]}}" class="{{['list_all',(keyword_show===1)?'red':'']}}" bindtap="__e">销量多</view></view><view data-event-opts="{{[['tap',[['e2',['$event']]]]]}}" hidden="{{!(show==true)}}" class="mask_black" bindtap="__e"></view><view class="{{['down_box',show===false?'mask_none':show===true?'mask_show':'']}}"><view data-event-opts="{{[['tap',[['chiose']]]]}}" class="down_list" bindtap="__e">全部</view><block wx:for="{{top_class}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view data-event-opts="{{[['tap',[['chiose',['$0','$1'],[[['top_class','id',item.id,'id']],[['top_class','id',item.id,'name']]]]]]]}}" class="down_list" bindtap="__e">{{''+item.name+''}}</view></block></view><view class="shopp_tab"><block wx:for="{{shopp_list}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="shopp_list"><view class="list_one"><image src="{{item.images}}" mode="widthFix"></image></view><view class="list_two">{{''+item.title+''}}</view><view class="list_three">{{'已售'+item.sales_count+''}}</view><view class="list_four">{{'￥'+item.price+''}}</view></view></block></view><uni-load-more vue-id="1" status="{{more}}" bind:__l="__l"></uni-load-more></view>