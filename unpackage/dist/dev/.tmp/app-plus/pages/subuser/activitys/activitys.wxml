<view class="content"><view class="status_bar"></view><view class="top"><view data-event-opts="{{[['tap',[['returns']]]]}}" class="top_enlarge" bindtap="__e"><image src="/static/image/com_page/returns.png" mode="widthFix"></image></view><text>我的活动</text><text></text></view><view class="allorder"><text data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="{{['one',(cur==0)?'active':'']}}" bindtap="__e">近期活动</text><text data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="{{['two',(cur==1)?'active':'']}}" bindtap="__e">往期活动</text></view><block wx:for="{{activity_Data.unexpired}}" wx:for-item="item" wx:for-index="index"><view data-event-opts="{{[['tap',[['$jump',['./tessera?id='+item.id]]]]]}}" hidden="{{!(cur==0)}}" class="quan" bindtap="__e"><image src="{{APIconfig.api_img+item.cover}}" mode="widthFix"></image><view class="q_right"><view>{{item.title}}</view><view>{{"￥"+item.price}}</view></view></view></block><block wx:for="{{activity_Data.expired}}" wx:for-item="item" wx:for-index="index"><view hidden="{{!(cur==1)}}" class="quan"><image src="{{APIconfig.api_img+item.cover}}" mode="widthFix"></image><view class="q_right"><view>{{item.title}}</view><view>{{"￥"+item.price}}</view></view></view></block></view>