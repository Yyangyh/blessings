<view class="content"><view class="status_bar"></view><view class="top"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="top_enlarge" bindtap="__e"><image src="/static/image/com_page/returns.png" mode="widthFix"></image></view><text>我的证书</text><text data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" bindtap="__e">{{show?'完成':'编辑'}}</text></view><block wx:for="{{data_lsit}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="line"><view class="l_left"><view hidden="{{!(show)}}" class="tab_left" style="transform:scale(0.8);"><label data-event-opts="{{[['tap',[['singleElection',[index]]]]]}}" class="radio" bindtap="__e"><radio value="r1" checked="{{item.choice}}"></radio></label></view><view>{{''+item.text+''}}</view></view><block wx:if="{{item.status==1}}"><view data-event-opts="{{[['tap',[['$jump',['./certificate?id='+item.tid]]]]]}}" class="l_right" bindtap="__e"><text>查看</text><image src="../../static/image/index/go.png"></image></view></block><block wx:else><block wx:if="{{item.status==0}}"><view class="l_right"><text>审核中</text></view></block><block wx:else><block wx:if="{{item.status==-1}}"><view class="l_right"><text>被驳回</text></view></block></block></block></view></block><view class="{{['tab_delete',show?'show':'Noshow']}}"><view class="alls"><label data-event-opts="{{[['tap',[['allElection']]]]}}" class="radio" bindtap="__e"><radio value="r1" checked="{{checked}}"></radio>全选</label></view><button data-event-opts="{{[['tap',[['deletes']]]]}}" bindtap="__e">删除</button></view></view>