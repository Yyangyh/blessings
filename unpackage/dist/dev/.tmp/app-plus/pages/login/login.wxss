  font-size: 28rpx;
  text-align: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.agreement view {
  border-bottom: 2rpx solid #fff;
}
.agreement text {
  display: inline-block;
  width: 4rpx;
  background: #fff;
  margin: 0 20rpx;
}
.Mask {
  position: fixed;
  left: 0;
  top: 0;
  height: 100%;
  width: 100%;
  z-index: 888;
  background: rgba(0, 0, 0, 0.6);
}
.treaty_box {
  position: fixed;
  z-index: 999;
  height: 800rpx;
  width: 80%;
  background: #fff;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.treaty_box image {
  z-index: 98;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
}
.treaty_box .t_conent {
  position: absolute;
  width: 100%;
  height: 800rpx;
  top: 0;
  left: 0;
  z-index: 100;
  padding: 20rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.treaty_box .t_conent .box_top {
  text-align: center;
  font-size: 36rpx;
}
.treaty_box .t_conent .box_conent {
  margin: 20rpx 0;
  height: 600rpx;
}
.treaty_box .t_conent button {
  width: 45%;
  font-size: 30rpx;
  margin: 0;
}
.treaty_box .t_conent .all_btn {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  position: absolute;
  width: 90%;
  height: 80rpx;
  left: 50%;
  bottom: 20rpx;
  margin: 0;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.treaty_box .t_conent .all_btn .agree {
  background: yellow;
  color: #000;
}
.agreement_box {
  position: fixed;
  left: 0;
  top: 0;
  height: 100%;
  width: 100%;
  z-index: 888;
  background: rgba(0, 0, 0, 0.6);
}
.agreement_box .elastic_agreement {
  position: absolute;
  z-index: 999;
  width: 85%;
  background: #fff;
  top: 50%;
  left: 50%;
  border-radius: 20rpx;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  padding: 30rpx 0;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.agreement_box .elastic_agreement .agreement_one {
  text-align: center;
  margin-bottom: 10rpx;
}
.agreement_box .elastic_agreement .agreement_two {
  font-size: 32rpx;
  padding: 0 30rpx 30rpx;
}
.agreement_box .elastic_agreement .btm {
  border-top: 1rpx solid #333333;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.agreement_box .elastic_agreement .btm .btm_two {
  color: #1D74FF;
}
.agreement_box .elastic_agreement .btm view {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: center;
  padding-top: 30rpx;
}
.agreement_box .elastic_agreement text {
  color: #1D74FF;
}

