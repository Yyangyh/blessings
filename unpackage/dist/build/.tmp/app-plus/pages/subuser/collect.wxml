<view class="content"><view class="status_bar"></view><view class="top"><image src="/static/image/com_page/returns.png" mode="widthFix" data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" bindtap="__e"></image><text>收藏</text><text data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" bindtap="__e">{{show?'完成':'编辑'}}</text></view><view class="allorder"><text data-event-opts="{{[['tap',[['choise',[1]]]]]}}" class="{{['one',(cur==1)?'active':'']}}" bindtap="__e">课程视频</text><text data-event-opts="{{[['tap',[['choise',[2]]]]]}}" class="{{['two',(cur==2)?'active':'']}}" bindtap="__e">音频</text><text data-event-opts="{{[['tap',[['choise',[3]]]]]}}" class="{{['three',(cur==3)?'active':'']}}" bindtap="__e">文章</text><text data-event-opts="{{[['tap',[['choise',[4]]]]]}}" class="{{['four',(cur==4)?'active':'']}}" bindtap="__e">商品</text></view><view hidden="{{!(cur==1||cur==2)}}" class="boxs"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="video_id"><view class="line"><view hidden="{{!(show)}}" class="tab_left" style="transform:scale(0.8);"><label data-event-opts="{{[['tap',[['singleElection',[index]]]]]}}" class="radio" bindtap="__e"><radio value="r1" checked="{{item.choice}}"></radio></label></view><view data-event-opts="{{[['tap',[['$jump',['/pages/com_page/video_details?id='+item.video_id+'&type='+item.type]]]]]}}" bindtap="__e"><image class="l_left" src="{{$api_img+item.v_pic}}" mode="scaleToFill"></image></view><view data-event-opts="{{[['tap',[['$jump',['/pages/com_page/video_details?id='+item.video_id+'&type='+item.type]]]]]}}" class="l_right" bindtap="__e"><view>{{item.long_title}}</view><view class="middle">{{item.view+"次观看"}}</view><view>￥99</view></view></view></block></view><view hidden="{{!(cur==3)}}" class="boxs"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="line"><view hidden="{{!(show)}}" class="tab_left" style="transform:scale(0.8);"><label data-event-opts="{{[['tap',[['singleElection',[index]]]]]}}" class="radio" bindtap="__e"><radio value="r1" checked="{{item.choice}}"></radio></label></view><view data-event-opts="{{[['tap',[['$jump',['/pages/subindex/article?id='+item.id]]]]]}}" bindtap="__e"><image class="l_left" src="{{$api_img+item.images}}" mode="scaleToFill"></image></view><view data-event-opts="{{[['tap',[['$jump',['/pages/subindex/article?id='+item.id]]]]]}}" class="l_right" bindtap="__e"><view>{{item.title}}</view><view class="article">{{item.access_count+"次阅读"}}</view></view></view></block></view><view hidden="{{!(cur==4)}}" class="boxs"><view class="box_s"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="commodity"><image src="{{$api_img+item.images_old}}" mode="scaleToFill" data-event-opts="{{[['tap',[['$jump',['/pages/subhome/details?id='+item.goods_id]]]]]}}" bindtap="__e"></image><view class="texts"><view class="t_name">{{item.title}}</view><view class="t_sell">{{"已售"+item.sales_count+"件"}}</view><view class="t_price">{{"￥"+item.min_price}}</view><view hidden="{{!(show)}}" class="tab_left" style="transform:scale(0.8);"><label data-event-opts="{{[['tap',[['singleElection',[index]]]]]}}" class="radio" bindtap="__e"><radio value="r1" checked="{{item.choice}}"></radio></label></view></view></view></block></view></view><view class="{{['tab_delete',show?'show':'Noshow']}}"><view class="alls"><label data-event-opts="{{[['tap',[['allElection']]]]}}" class="radio" bindtap="__e"><radio value="r1" checked="{{checked}}"></radio>全选</label></view><button data-event-opts="{{[['tap',[['deletes']]]]}}" bindtap="__e">删除</button></view><uni-load-more vue-id="1" status="{{more}}" bind:__l="__l"></uni-load-more></view>