<view class="content"><view class="status_bar"></view><view class="top"><image src="/static/image/com_page/returns.png" mode="widthFix" data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" bindtap="__e"></image><text>收藏</text><text data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" bindtap="__e">{{show?'完成':'编辑'}}</text></view><view class="allorder"><text data-event-opts="{{[['tap',[['choise',[1]]]]]}}" class="{{['one',(cur==1)?'active':'']}}" bindtap="__e">课程视频</text><text data-event-opts="{{[['tap',[['choise',[2]]]]]}}" class="{{['two',(cur==2)?'active':'']}}" bindtap="__e">音频</text><text data-event-opts="{{[['tap',[['e2',['$event']]]]]}}" class="{{['three',(cur==3)?'active':'']}}" bindtap="__e">文章</text><text data-event-opts="{{[['tap',[['e3',['$event']]]]]}}" class="{{['four',(cur==4)?'active':'']}}" bindtap="__e">商品</text></view><view class="boxs"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="line"><view hidden="{{!(show)}}" class="tab_left" style="transform:scale(0.8);"><label data-event-opts="{{[['tap',[['singleElection',[index]]]]]}}" class="radio" bindtap="__e"><radio value="r1" checked="{{item.choice}}"></radio></label></view><image class="l_left" src="{{item.v_pic}}" mode="scaleToFill"></image><view class="l_right"><view>{{item.long_title}}</view><view class="middle">{{item.view+"次观看"}}</view><view>￥99</view></view></view></block></view><view hidden="{{!(cur==4)}}" class="boxs"><view class="box_s"><view class="commodity"><image src="../../static/image/index/page.png" mode="scaleToFill"></image><view class="texts"><view class="t_name">管理魂丨家庭式管理系统</view><view class="t_sell">已售100件</view><view class="t_price">￥2980.0</view></view></view></view></view><view class="{{['tab_delete',show?'show':'Noshow']}}"><view class="alls"><label data-event-opts="{{[['tap',[['allElection']]]]}}" class="radio" bindtap="__e"><radio value="r1" checked="{{checked}}"></radio>全选</label></view><button data-event-opts="{{[['tap',[['deletes']]]]}}" bindtap="__e">删除</button></view></view>