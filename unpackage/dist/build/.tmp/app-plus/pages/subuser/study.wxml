<view class="content"><view class="status_bar"></view><returns vue-id="1" titles="{{title}}" bind:__l="__l"></returns><view class="allorder"><text data-event-opts="{{[['tap',[['choise',[1]]]]]}}" class="{{['one',(cur==1)?'active':'']}}" bindtap="__e">课程视频</text><text data-event-opts="{{[['tap',[['choise',[2]]]]]}}" class="{{['two',(cur==2)?'active':'']}}" bindtap="__e">音频</text></view><view class="box"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view data-event-opts="{{[['tap',[['$jump',['../com_page/video_details?id='+item.id]]]]]}}" class="line" bindtap="__e"><image class="l_left" src="{{APIconfig.api_img+item.v_pic}}" mode="scaleToFill"></image><view class="l_right"><view>{{''+item.long_title}}</view><view class="middle">{{item.view+"次观看"}}</view><view>{{"已学习"+item.plan}}</view></view></view></block></view></view>