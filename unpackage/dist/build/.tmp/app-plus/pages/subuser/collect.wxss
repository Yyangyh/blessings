page {
  background-color: #FFFFFF;
}
.content {
  padding-top: calc(225rpx + var(--status-bar-height));
  padding-bottom: 120rpx;
}
.top {
  height: 105rpx;
  width: 100%;
  position: fixed;
  top: var(--status-bar-height);
  left: 0;
  background: #FFF;
  z-index: 888;
  padding: 0 40rpx;
  box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 32rpx;
  font-weight: bold;
  color: #333333;
  margin-bottom: 24rpx;
  background: #fff;
  box-shadow: 0px 2rpx 4rpx 0px rgba(0, 0, 0, 0.1);
}
.top image {
    height: 40rpx;
    width: 40rpx;
}
.top text:nth-of-type(2) {
    color: #666666;
    font-size: 28rpx;
    font-weight: 400;
    display: inline-block;
}
.allorder {
  width: 100%;
  height: 120rpx;
  position: fixed;
  top: calc(105rpx + var(--status-bar-height));
  z-index: 111;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  text-align: center;
  line-height: 120rpx;
  font-size: 32rpx;
  background-color: #F6F6F7;
}
.allorder text {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
}
.active {
  color: #D80000;
}
.line {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 30rpx;
  border-bottom: 1rpx solid #EEEEEE;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  background-color: #FFFFFF;
}
.line .l_left {
    width: 268rpx;
    height: 179rpx;
    margin-right: 30rpx;
}
.line .l_right {
    height: 179rpx;
    -webkit-box-flex: 2;
    -webkit-flex-grow: 2;
            flex-grow: 2;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
            flex-direction: column;
    /* justify-content: space-around; */
}
.line .l_right view:first-child {
      font-size: 24rpx;
}
.line .l_right .article {
      font-size: 24rpx !important;
      color: #999999 !important;
      margin-top: 10rpx;
}
.line .l_right .middle {
      font-size: 24rpx;
      color: #999999;
      margin: 10rpx 0;
}
.line .l_right view:last-child {
      font-size: 28rpx;
      color: #D80000;
      font-weight: 500;
}
.tab_delete {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  border-top: 2rpx solid #F1F1F1;
  padding: 0 40rpx;
  height: 120rpx;
  color: #666;
  background: #fff;
  -webkit-transform: translateY(100%);
          transform: translateY(100%);
  -webkit-transition: .3s;
  transition: .3s;
}
.tab_delete .alls {
    font-size: 34rpx;
}
.tab_delete .alls .radio {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
}
.tab_delete button {
    background: #D80000;
    color: #fff;
    width: 224rpx;
    height: 90rpx;
    line-height: 90rpx;
    border-radius: 90rpx;
    margin-left: 20rpx;
    margin-right: 0rpx;
}
.box_s {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.box_s .commodity {
    width: 50%;
    box-sizing: border-box;
    padding: 20rpx;
}
.box_s .commodity image {
      width: 341rpx;
      height: 341rpx;
}
.box_s .commodity .texts {
      margin-top: 25rpx;
      position: relative;
}
.box_s .commodity .texts .t_name {
        font-size: 24rpx;
        display: -webkit-box;
        -webkit-box-orient: vertical;
        -webkit-line-clamp: 2;
        overflow: hidden;
}
.box_s .commodity .texts .t_sell {
        font-size: 24rpx;
        color: #999999;
        margin: 15rpx 0;
}
.box_s .commodity .texts .t_price {
        font-size: 28rpx;
        color: #D80000;
}
.box_s .commodity .texts .tab_left {
        position: absolute;
        right: 0;
        bottom: 0;
}
.show {
  -webkit-transform: translateY(0);
          transform: translateY(0);
}
.Noshow {
  -webkit-transform: translateY(100%);
          transform: translateY(100%);
}

