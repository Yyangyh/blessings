<view class="content"><view class="status_bar"></view><view class="top"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="top_enlarge" bindtap="__e"><image src="/static/image/com_page/returns.png" mode="widthFix"></image></view><text>余额</text><text data-event-opts="{{[['tap',[['$jump',['/pages/subuser/threeuser/cash']]]]]}}" bindtap="__e">提现</text></view><image class="black" src="../../static/image/subuser/blance.png"></image><view class="money"><view>可用余额（元）</view><view>{{user.money}}</view></view><view class="allorder"><text data-event-opts="{{[['tap',[['choise']]]]}}" class="{{['one',(cur==2)?'active':'']}}" bindtap="__e">全部</text><text data-event-opts="{{[['tap',[['choise',[1]]]]]}}" class="{{['two',(cur==1)?'active':'']}}" bindtap="__e">收入</text><text data-event-opts="{{[['tap',[['choise',['0']]]]]}}" class="{{['three',(cur==0)?'active':'']}}" bindtap="__e">支出</text></view><view class="box"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="line"><view class="l_left"><view>{{item.title}}</view><view>{{item.time}}</view></view><view class="l_right">{{item.desc}}</view></view></block><uni-load-more vue-id="1" status="{{more}}" bind:__l="__l"></uni-load-more></view></view>