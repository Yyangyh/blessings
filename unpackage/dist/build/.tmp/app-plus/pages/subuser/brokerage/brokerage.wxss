page {
  background-color: #F6F6F7;
}
.content {
  width: 100%;
  background-color: #F6F6F7;
}
.content .worp {
    background-color: #FFFFFF;
    width: 100%;
}
.content .worp ._hr {
      height: 1rpx;
      border: none;
      border-top: 1px solid #F4F4F4;
}
.content .worp .top {
      background-color: #EF7C38;
      width: 100%;
      height: 160rpx;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
}
.content .worp .top .img_box {
        width: 100rpx;
        height: 100rpx;
        margin: 0 29rpx 0 34rpx;
        border-radius: 50%;
        overflow: hidden;
}
.content .worp .top image {
        height: 100%;
        width: 100%;
}
.content .worp .top .msg {
        font-size: 28rpx;
        color: #FFFFFF;
        -webkit-box-orient: vertical;
        -webkit-box-direction: normal;
        -webkit-flex-direction: column;
                flex-direction: column;
        -webkit-box-pack: justify;
        -webkit-justify-content: space-between;
                justify-content: space-between;
}
.content .worp .line {
      font-size: 24rpx;
      margin: 35rpx 0 35rpx 21rpx;
}
.content .worp .line text {
        margin-right: 5rpx;
}
.content .worp .line1 {
      font-size: 24rpx;
      padding: 30rpx 25rpx 30rpx 21rpx;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
}
.content .worp .line1 image {
        width: 36rpx;
        height: 36rpx;
}
.content .worp .line2 {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
      padding: 30rpx 25rpx;
}
.content .worp .line2 .money {
        color: #D80000;
        font-size: 32rpx;
        font-weight: 500rpx;
        margin: 10rpx;
}
.content .worp .line2 .textss {
        font-size: 24rpx;
        padding: 0 0 10rpx 0;
}
.content .worp .line2 .btn {
        width: 180rpx;
        height: 60rpx;
        background: #d80000;
        border-radius: 30rpx;
        text-align: center;
        color: #FFFFFF;
        line-height: 60rpx;
        font-size: 28rpx;
}
.content .classify {
    background: #FFFFFF;
    width: 100%;
    margin-top: 30rpx;
}
.content .classify .box {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
}
.content .classify .box .box-list {
        width: 50%;
        height: 161rpx;
        display: -webkit-box;
        display: -webkit-flex;
        display: flex;
        -webkit-box-align: center;
        -webkit-align-items: center;
                align-items: center;
        -webkit-box-pack: center;
        -webkit-justify-content: center;
                justify-content: center;
        font-size: 24rpx;
}
.content .classify .box .box-list image {
          width: 55rpx;
          height: 55rpx;
          margin-right: 10rpx;
}
.content .classify .box .box-list .toutcome {
          color: #D80000;
}

